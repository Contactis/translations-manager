{
  "name": "Account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "display": [
    "id",
    "username",
    "password",
    "email",
    "firstName",
    "lastName",
    {
      "property": "roleMapping",
      "sourceModel": "Account",
      "sourceKey": "accountId",
      "label": "Role",
      "type": "reference",
      "roles": [
        "admin"
      ],
      "options": {
        "model": "Role",
        "key": "id",
        "relationship": "Roles",
        "searchField": "description",
        "placeholder": "Select Roles",
        "multiple": true,
        "allowInsert": false,
        "matchTemplate": "{{ $item.description }}",
        "choiceTemplate": "{{ item.description }}",
        "junctionMeta": {
          "principalType": "USER"
        }
      }
    },
    "created",
    "lastUpdated"
  ],
  "properties": {
    "id": {
      "type": "number",
      "display": {
        "label": "ID"
      }
    },
    "realm": {
      "type": "string"
    },
    "username": {
      "type": "string",
      "length": 128,
      "display": {
        "label": "Username"
      }
    },
    "password": {
      "type": "string",
      "required": true,
      "length": 65534,
      "display": {
        "label": "Password",
        "type": "password",
        "description": "Password should contain at least 1 upper case letter, 1 number, and at least 6 characters long."
      }
    },
    "credentials": {
      "type": "string",
      "deprecated": true,
      "length": 65534
    },
    "challenges": {
      "type": "object",
      "deprecated": true,
      "length": 65534
    },
    "email": {
      "type": "string",
      "required": true,
      "length": 254,
      "display": {
        "label": "E-mail"
      }
    },
    "emailVerified": {
      "type": "boolean"
    },
    "verificationToken": {
      "type": "string",
      "length": 512
    },
    "status": {
      "type": "string",
      "length": 512
    },
    "created": {
      "type": "date",
      "display": {
        "label": "Created Date",
        "readonly": true,
        "evalDefault": "new Date()"
      }
    },
    "lastUpdated": {
      "type": "date",
      "display": {
        "label": "Last Updated",
        "readonly": true,
        "evalDefault": "new Date()",
        "forceDefaultOnSave": true
      }
    },
    "firstName": {
      "type": "string",
      "length": 128,
      "display": {
        "label": "First Name"
      }
    },
    "lastName": {
      "type": "string",
      "length": 256,
      "display": {
        "label": "Last Name"
      }
    },
    "role": {
      "type": "string",
      "length": 128,
      "display": {
        "label": "Role"
      }
    },
    "interfaceLanguage": {
      "type": "string",
      "length": 40,
      "display": {
        "label": "User interface language"
      }
    }
  },
  "validations": [],
  "relations": {
    "RoleMappings": {
      "type": "hasMany",
      "model": "RoleMapping",
      "foreignKey": "principalId"
    },
    "Roles": {
      "type": "hasMany",
      "model": "Role",
      "through": "RoleMapping"
    }
  },
  "acls": [],
  "methods": {}
}
